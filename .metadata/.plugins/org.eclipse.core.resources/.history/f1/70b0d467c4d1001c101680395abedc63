package p290;

public class DmbCellPhone extends CellPhone{
	//필드
	int channel1;
	
	//생성자
	DmbCellPhone(String model, String color, int channel){
		this.model = model;
		this.color = color;
		channel = channel;
	}
	
	//매소드
	void turnOnDmb() {
		System.out.println("채널"+channel1+"번 DMB 방송 수신을 시작합니다");
	}
	
	void changeChannelDmb(int channel) {
		channel1 = channel;
		System.out.println("채널"+channel1+"번으로 바꿉니다");
	}
	void turnOffDmb(){
		System.out.println("DMB 방송 수신을 멈춥니다.");
	}
}
