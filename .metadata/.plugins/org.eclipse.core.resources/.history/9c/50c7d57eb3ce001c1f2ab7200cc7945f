package chap05;

public class ArrayCreateByValueListExample {

	public static void main(String[] args) {
		// 값 목록으로 배열 생성
		int[] scores= {83,90,97};
		
		System.out.println("scores[0]:"+scores[0]);
		System.out.println("scores[1]:"+scores[1]);
		System.out.println("scores[2]:"+scores[2]);
		}
	}

}
