package chap05;

public class ArrayCreatByExample {

	public static void main(String[] args) {
		// new 연산자로 배열 생성
		int[] arr1 = new int[3];
		for(int i=0;i<3;i++) {
			//System.out.println("arr["+i+"] : "+arr1); 메모리 주소 출력?
			System.out.println("arr["+i+"] : "+arr1[i]);
		}
		
		arr1[0] = 10;
		arr1[1] = 20;
		arr1[2] = 30;
		for(int i=0;i<3;i++) {
			System.out.println("arr["+i+"] : "+arr1[i]);
	    }
		
		double[] arr2 = new double[3];
		for(int i=0;i<3;i++) {
			System.out.println("arr["+i+"] : "+arr2[i]);
		}
  }
}
