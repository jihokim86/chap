package chap05;

import java.util.Iterator;

public class ArrayLengthExample {

	public static void main(String[] args) {
		//¹è¿­ÀÇ length ÇÊµå
		int[] scores = {83,90,87};
		int sum=0;
		for(int i =0; i<scores.length;i++) {
			sum += scores[i];
		}
		System.out.println("ÃÑÇÕ : "+ sum);
		System.out.println("Æò±Õ : "+ (double)sum/3);
	}

}
